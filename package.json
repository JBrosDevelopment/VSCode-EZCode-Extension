{
  "name": "ezcode",
  "displayName": "EZCode",
  "description": "EZCode is a comprehensive programming language built off of Microsoft WinForms. EZCode strives to make it as easy as possible to build a Windows program ranging from a Visual Application to simple console programs.",
  "version": "3.0.0",
  "publisher": "JBrosDevelopment",
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/JBrosDevelopment/VSCode-EZCode-Extension.git"
  },
  "icon": "icons/EZCode_Logo.png",
  "contributes": {
    "languages": [{
      "id": "ezcode",
      "aliases": ["EZCode", "ezcode"],
      "extensions": [".ezcode"],
      "configuration": "./language-configuration.json"
    },
    {
      "id": "ezproj",
      "aliases": ["EZProj", "ezproj"],
      "extensions": [".ezproj"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "ezcode",
      "scopeName": "source.ezcode",
      "path": "./syntaxes/ezcode.tmLanguage.json"
    },
    {
      "language": "ezproj",
      "scopeName": "source.ezproj",
      "path": "./syntaxes/ezproj.tmLanguage.json"
    }]
  }
}
